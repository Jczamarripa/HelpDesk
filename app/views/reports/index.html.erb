<div style="width:80%;margin:0 auto;">
<h1 style="text-align:center;">Reportes Recibidos</h1>
<br>
<% @reports.each do |report| %>
<div class="field form-control"><%=link_to "Problema: #{report.problem}",edit_report_path(report.id)%></div>
